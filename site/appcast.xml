---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:sparkle="http://www.andymatuschak.org/xml-namespaces/sparkle"  xmlns:dc="http://purl.org/dc/elements/1.1/">
  <channel>
    <title>Noise Changelog</title>
    <link>http://quamen.github.com/noise/appcast.xml</link>
    <description>Most recent changes with links to updates.</description>
    <language>en</language>
    {% for post in site.posts %}
      <item>
        <title>{{ post.title }}</title>
        <sparkle:releaseNotesLink>
          http://quamen.github.com{{ post.url }}
        </sparkle:releaseNotesLink>
        <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
        <enclosure
          url="http://cloud.github.com/downloads/quamen/noise/noise-{{ post.release-version }}.zip"
          sparkle:version="{{ post.release-version }}"
          length="{{ post.release-length }}"
          type="application/octet-stream"
          sparkle:dsaSignature="{{ post.release-signature }}" />
      </item>
    {% endfor %}
  </channel>
</rss>
